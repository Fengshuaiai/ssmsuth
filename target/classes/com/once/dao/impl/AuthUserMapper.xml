<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.once.dao.IAuthUserDao">
    <!-- 根据id查询 user-->
    <select id="getAuthUser" parameterType="com.once.pojo.AuthUser" resultType="com.once.pojo.AuthUser">
    select  name ,pwd  from Auth_User  where  name =#{name} and pwd =#{pwd}
  </select>

</mapper>